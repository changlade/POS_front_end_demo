{
  "name": "danone-scout",
  "version": "1.0.0",
  "description": "Danone Scout - Mobile app for Danone employees to scout and track menu items in Paris businesses",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-leaflet": "^4.2.1",
    "leaflet": "^1.9.3",
    "styled-components": "^5.3.6",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t danone-scout .",
    "docker:run": "docker run -d -p 3000:80 --name danone-scout danone-scout",
    "docker:stop": "docker stop danone-scout && docker rm danone-scout",
    "docker:logs": "docker logs danone-scout",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
